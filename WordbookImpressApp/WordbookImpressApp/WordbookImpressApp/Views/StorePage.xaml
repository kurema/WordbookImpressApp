        <ToolbarItem Order="Secondary" Text="利用時の注意" Clicked="ToolbarItem_Clicked_Attention" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <ScrollView>
            <StackLayout x:Name="StoreStack" Spacing="0" />
        </ScrollView>
    </ContentPage.Content>
</ContentPage>